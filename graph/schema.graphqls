# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Property {
  _id: ID!
  price: Int!
  location: String!
  url: String!
  daftId: Int!
  entered: String!
  views: Int!
  type: String!
  bathroom: Int!
  bed: Int!
}
type Query {
  property(_id: String!): Property!
  properties: [Property!]!
}
