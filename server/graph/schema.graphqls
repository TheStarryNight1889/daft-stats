# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Property {
  _id: ID!
  price: Int!
  location: String!
  url: String!
  entered: String!
  views: Int!
  type: String!
  daft_id: Int!
  bathroom: Int!
  bed: Int!
  removed: String!
}
type Stat {
  _id: ID!
  timestamp: String!
  price_average: Float!
  price_high: Float!
  price_low: Float!
  price_distribution: [Int!]!
}
type Query {
  property(daft_id: Int!): Property!
  properties: [Property!]!

  stats: [Stat!]!
}
