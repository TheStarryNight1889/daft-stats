{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:vue/recommended",
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        // lint on save
        "no-console": "off"
    }
}
